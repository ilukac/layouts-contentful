{
    "name": "netgen/block-manager-contentful",
    "description": "Netgen Layouts & Contentful integration",
    "license": "proprietary",
    "authors": [
        {
            "name": "Netgen",
            "homepage": "https://www.netgenlabs.com"
        }
    ],
    "require": {
        "netgen/block-manager": "~0.12.0",
        "contentful/contentful-bundle": "^3.0",
        "doctrine/orm": "^2.6",
        "symfony-cmf/routing-bundle": "^2.1",
        "aptoma/twig-markdown": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "symfony/phpunit-bridge": "^4.1",
        "phpstan/phpstan": "^0.10",
        "phpstan/phpstan-phpunit": "^0.10",
        "matthiasnoback/symfony-dependency-injection-test": "^3.0",
        "franzl/studio": "*"
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit --colors=always",
        "coverage": "@php -dzend_extension=xdebug.so vendor/bin/phpunit --coverage-html=coverage --colors=always",
        "phpstan": "@php vendor/bin/phpstan analyse -c phpstan.neon --level=7 --ansi lib bundle",
        "phpstan-tests": "@php vendor/bin/phpstan analyse -c phpstan.tests.neon --level=7 --ansi tests"
    },
    "repositories": [
        { "type": "composer", "url": "https://packagist.netgen.biz" }
    ],
    "autoload": {
        "psr-4": {
            "Netgen\\BlockManager\\Contentful\\": "lib/",
            "Netgen\\Bundle\\ContentfulBlockManagerBundle\\": "bundle/"
        }
    },
    "autoload-dev": {
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ],
        "psr-4": {
            "Netgen\\BlockManager\\Contentful\\Tests\\": "tests/lib/",
            "Netgen\\Bundle\\ContentfulBlockManagerBundle\\Tests\\": "tests/bundle/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.12.x-dev"
        }
    }
}
